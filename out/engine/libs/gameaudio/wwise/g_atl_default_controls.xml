<ATLConfig atl_name="g_atl_default_controls">
 <AudioTriggers>
  <ATLTrigger atl_name="do_nothing"/>
  <ATLTrigger atl_name="get_focus">
   <WwiseEvent wwise_name="UnMute_all_g_default_controls"/>
  </ATLTrigger>
  <ATLTrigger atl_name="lose_focus">
   <WwiseEvent wwise_name="Mute_all_g_default_controls"/>
  </ATLTrigger>
  <ATLTrigger atl_name="mute_all">
   <WwiseEvent wwise_name="Mute_all_g_default_controls"/>
  </ATLTrigger>
  <ATLTrigger atl_name="unmute_all">
   <WwiseEvent wwise_name="UnMute_all_g_default_controls"/>
  </ATLTrigger>
 </AudioTriggers>
 <AudioRtpcs>
  <ATLRtpc atl_name="SS_Air_Turbulence" path="wwise_ss">
   <WwiseRtpc wwise_name="SS_Air_Turbulence"/>
  </ATLRtpc>
  <ATLRtpc atl_name="SS_Air_TimeOfDay" path="wwise_ss">
   <WwiseRtpc wwise_name="SS_Air_TimeOfDay"/>
  </ATLRtpc>
  <ATLRtpc atl_name="SS_Air_Storm" path="wwise_ss">
   <WwiseRtpc wwise_name="SS_Air_Storm"/>
  </ATLRtpc>
  <ATLRtpc atl_name="SS_Air_Size" path="wwise_ss">
   <WwiseRtpc wwise_name="SS_Air_Size"/>
  </ATLRtpc>
  <ATLRtpc atl_name="SS_Air_RPM" path="wwise_ss">
   <WwiseRtpc wwise_name="SS_Air_RPM"/>
