  <ATLRtpc atl_name="SS_Air_Presence" path="wwise_ss">
   <WwiseRtpc wwise_name="SS_Air_Presence"/>
  </ATLRtpc>
  <ATLRtpc atl_name="SS_Air_Month" path="wwise_ss">
   <WwiseRtpc wwise_name="SS_Air_Month"/>
  </ATLRtpc>
  <ATLRtpc atl_name="SS_Air_Fury" path="wwise_ss">
   <WwiseRtpc wwise_name="SS_Air_Fury"/>
  </ATLRtpc>
  <ATLRtpc atl_name="SS_Air_Freefall" path="wwise_ss">
