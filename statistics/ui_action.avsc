{
    "name": "ui_action",
    "doc": "Origin: ui. Emitter: client. Indicates when navigation action was triggered on client",
    "type": "record",
    "fields": [
        {
            "name": "action_name",
            "type": "string",
            "default": "-1",
            "doc": "action name"
        },
        {
            "name": "ui_element_path",
            "type": "string",
            "default": "-1",
            "doc": "name of acted element"
        },
        {
            "name": "ui_context_info",
            "type": "string",
            "default": "-1",
            "doc": "additional contextual info, used mostly for generic elements, when ui_element_path is not sufficient"
        },
        {
            "name": "name",
            "type": "string",
            "default": "-1",
            "doc": "unique name of the event"
        },
        {
            "name": "pid",
            "type": "long",
            "default": -1,
            "doc": "namespace-unique identifier of a player"
        },
        {
            "name": "ts",
            "type": "string",
            "default": "-1",
            "doc": "ISO8601 UTC timestamp of the event invocation"
        },
        {
            "name": "gid",
            "type": "string",
            "default": "-1",
            "doc": "namespace-unique identifier of the match"
        },
        {
            "name": "sid",
            "type": "string",
            "default": "-1",
            "doc": "global-unique identifier of the player's login session (time between login and logout)"
        },
        {
            "name": "ns",
            "type": "string",
            "default": "-1",
            "doc": "namespace of the server's deployment (realm)"
        }
    ]
}